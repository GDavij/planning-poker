{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=WebApiDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Cors": {
    "Origins": "http://localhost:5173",
    "Headers": [
      "Accept",
      "Authorization",
      "Content-Type",
      "Origin",
      "X-Requested-With",
      "x-signalr-user-agent",
      "X-API-Key",
      "Cache-Control",
      "If-None-Match",
      "If-Match",
      "X-Correlation-ID",
      "X-CSRF-Token",
      "Retry-After"
    ],
    "Methods": [
      "GET",
      "POST",
      "PUT",
      "DELETE",
      "OPTIONS",
      "PATCH",
      "CONNECT"
    ]
  }
}
